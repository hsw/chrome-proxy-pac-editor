{
  "manifest_version": 2,
  "version": "0.2",
  "name": "Proxy.pac editor",
  "description": "Edit proxy.pac configuration",
  "icons": {
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon16.png",
      "24": "img/icon24.png",
      "32": "img/icon32.png"
    },
    "default_popup": "popup.html",
    "default_title": "Proxy.pac"
  },
  "permissions": [
    "proxy"
  ]
}
